<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <IncludeSymbols>true</IncludeSymbols>
    <SymbolPackageFormat>snupkg</SymbolPackageFormat>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Daniele Fusi</Authors>
    <Company>Fusi</Company>
    <Product>Cadmus</Product>
    <Description>Cadmus components for the Inquisition Graffiti project.</Description>
    <NeutralLanguage>en-US</NeutralLanguage>
    <Version>1.1.3</Version>
    <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
    <Copyright>by Daniele Fusi</Copyright>
    <PackageTags>Cadmus;ingra parts</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Cadmus.Core" Version="2.5.2" />
    <PackageReference Include="Cadmus.Itinera.Parts" Version="1.2.0" />
    <PackageReference Include="Fusi.Antiquity" Version="1.2.0" />
  </ItemGroup>

</Project>
